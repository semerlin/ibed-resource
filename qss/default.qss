/* infusion widget */
QWidget#widgetLeft, QWidget#widgetSpeed{
	min-width: 230px;
	max-width: 230px;
	min-height: 230px;
	max-height: 230px;
	border-radius: 115px;
	background-color: rgb(21, 53, 103);
}

CircleProgressBar#progressBarLeft{
	qproperty-active: rgb(234, 210, 36);
	qproperty-background: rgb(91, 103, 129);
	qproperty-circleHeight: 6;
	qproperty-startAngle: 0;
	qproperty-innerRadius: 72;
	qproperty-fontSize: 61;
	qproperty-fontFamily: "冬青黑體簡體中文";
}

CircleProgressBar#progressBarSpeed{
	qproperty-active: rgb(255, 0, 0);
	qproperty-background: rgba(0, 0, 0, 0);
	qproperty-circleHeight: 22;
	qproperty-dashWidth: 3;
	qproperty-intervalAngle: 5;
	qproperty-startAngle: 0;
	qproperty-innerRadius: 70;
	qproperty-fontSize: 61;
	qproperty-fontFamily: "冬青黑體簡體中文";
	qproperty-startColor: rgb(0, 85, 255);
	qproperty-stopColor: rgb(255, 0, 0);
}

InfusionWidget QLabel{
	font-size: 20px;
	color: white;
}

InfusionWidget QPushButton{
	font-size: 20px;
	color: white;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			stop: 0 rgb(149, 184, 214), stop: 1 rgb(66, 143, 195));
	border: 1px solid white;
	border-radius: 5px;
	min-height: 45px;
	min-width: 145px;
}

InfusionWidget QPushButton:!enabled{
	font-size: 20px;
	color: black;
	background-color: rgb(101, 134, 177);
	border: 1px solid rgb(45, 73, 121);
	border-radius: 5px;
	min-height: 45px;
	min-width: 145px;
}

InfusionWidget QPushButton:pressed{
	font-size: 20px;
	color: white;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			stop: 0 rgb(14, 46, 93), stop: 1 rgb(52, 145, 209));
	border: 1px solid white;
	border-radius: 5px;
	min-height: 45px;
	min-width: 145px;
}


InfusionWidget QComboBox{
	font-size: 20px;
	border: 1px solid rgb(45, 73, 121);
	border-radius: 5px;
	padding: 1px 18px 1px 3px;
	min-height: 30px;
	background-color: rgb(101, 134, 177);
}

InfusionWidget QComboBox:on{ /* shift the text when the popup opens */
	padding-top: 3px;
	padding-left: 4px;
}

InfusionWidget QComboBox::drop-down{
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 25px;
	border-left-width: 1px;
	border-left-color: rgb(45, 73, 121);
	border-left-style: solid; /* just a single line */
	border-top-right-radius: 5px; /* same radius as the QComboBox */
	border-bottom-right-radius: 5px;
}

InfusionWidget QComboBox::down-arrow{
	image: url(:/res/arrow.png);
}

InfusionWidget QComboBox QAbstractItemView{
	border: 1px solid rgb(45, 73, 121);
	selection-background-color: lightgray;
	background-color: rgb(101, 134, 177);
}

InfusionWidget QComboBox::down-arrow:on{ /* shift the arrow when popup is open */
	top: 1px;
	left: 1px;
}

