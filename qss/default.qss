/* Input Method*/
QFrame#frameNumIPMethod{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			stop: 0 rgb(255, 255, 255), stop: 1 rgb(122, 169, 219));
}

QFrame#frameNumIPMethod QPushButton{
	font-size: 22px;
	color: white;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			stop: 0 rgb(145, 187, 215), stop: 1 rgb(19, 107, 163));
	border: 1px solid rgb(192, 212, 225);
}

QFrame#frameNumIPMethod QPushButton:pressed{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
			stop: 0 rgb(14, 46, 93), stop: 1 rgb(52, 145, 209));
}

