	border-top: 0px solid rgb(178, 178, 178);
	border-left: 0px solid rgb(178, 178, 178);
	border-right: 1px solid rgb(178, 178, 178);
	border-bottom: 0px solid rgb(178, 178, 178);
}

QTableView#longAdvise QHeaderView::section{
	color: white;
	font-size: 12px;
	min-height: 28px;
	background-color: rgb(0, 96, 169);
	border-top: 0px solid rgb(178, 178, 178);
	border-left: 0px solid rgb(178, 178, 178);
	border-right: 1px solid rgb(178, 178, 178);
	border-bottom: 0px solid rgb(178, 178, 178);
}

/* vertical scrollbar */
QTableView QScrollBar:vertical{
	background: rgb(175, 175, 175);
	width: 5px;
	margin: 0px 0 0px 0;
}

QTableView QScrollBar::handle:vertical{
	border-radius: 2px;
	background: rgb(66, 66, 66);
	min-height: 20px;
	margin: 1px; 
}

QTableView QScrollBar::add-line:vertical{
	height: 0px;
}

QTableView QScrollBar::sub-line:vertical{
	height: 0px;
}
  
QtableView QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical{
	background: none;
}

QTableView QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
}

/**/
QTableView QScrollBar:horizontal{
	background: rgb(175, 175, 175);
	height: 5px;
	margin: 0px 0 0px 0;
}

QTableView QScrollBar::handle:horizontal{
	border-radius: 2px;
	background: rgb(66, 66, 66);
	min-width: 20px;
	margin: 1px; 
}

QTableView QScrollBar::add-line:horizontal{
	width: 0px;
}

QTableView QScrollBar::sub-line:horizontal{
	width: 0px;
}
  
QtableView QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal{
	background: none;
}

QTableView QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	background: none;
}


